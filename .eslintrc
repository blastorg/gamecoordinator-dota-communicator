{
    "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [".js", ".ts", ".d.ts"]
        }
      }
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2020
    },
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "ignorePatterns": ["protobufs/generated/*", "lib/*"]
  }